    </inertial>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry name="segway_base_geom">
        <mesh filename="package://segway_robot/meshes/segway_base.stl" />
      </geometry>
      <material name="Blue" />
    </visual>    

    <collision>
